(setq org-todo-keyword-faces
      '(("DELEGATED" . shadow)
        ("STARTED" . org-warning)
        ("FEEDBACK" . (:foreground "yellow"))
        ("BLOCKED" . shadow)
        ("REWORK" . org-warning)
        ("VERIFY" . org-warning)
        ("CANCELED" . (:foreground "red" :weight bold :strike-through t))))
